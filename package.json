{
  "name": "ts-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc-alias",
    "start:dev": "NODE_ENV=development nodemon --exec npx ts-node -r tsconfig-paths/register ./src/server.ts",
    "start:prod": "NODE_ENV=production node ./dist/server.js",
    "migrate:create": "cross-env NAME=$NAME node ./src/database/migrate create --name $NAME --folder src/database/migrations",
    "migrate:up": "node -r tsconfig-paths/register ./src/database/migrate up",
    "migrate:down": "node -r tsconfig-paths/register ./src/database/migrate down",
    "migrate:revert-all": "node ./src/database/migrate down --to 0",
    "utils:generate-key": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.11",
    "@types/validator": "^13.12.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mariadb": "^3.3.1",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "tsconfig-paths": "^4.2.0",
    "umzug": "^3.8.1"
  }
}
